<center>
  <table id="hnmain" border="0" cellpadding="0" cellspacing="0" width="85%" bgcolor="#f6f6ef">
    <tr>
      <td bgcolor="#ff6600">
        <table border="0" cellpadding="0" cellspacing="0" width="100%" style="padding:2px">
          <tr>
            <td style="width:18px;padding-right:4px">
              <a href="https://news.ycombinator.com">
                <img src="https://news.ycombinator.com/y18.gif" width="18" height="18" style="border:1px white solid;">
              </a>
            </td>
            <td style="line-height:12pt; height:10px;">
              <span class="pagetop">
                <b class="hnname"><a href="news">Hacker News</a></b>
                <a href="newest">new</a>
                | <a href="newcomments">comments</a>
                | <a href="show">show</a>
                | <a href="ask">ask</a>
                | <a href="jobs">jobs</a>
                | <a href="submit">submit</a>
              </span>
            </td>
            <td style="text-align:right;padding-right:4px;">
              <span class="pagetop">
                <a href="#">login</a>
              </span>
            </td>
          </tr>
        </table>
      </td>
    </tr>
    <tr style="height:10px"></tr>
    <tr>
      <td>
        <table border="0" cellpadding="0" cellspacing="0" class="itemlist">
          <!--
          <tr class='athing' id='16851021'>
            <td align="right" valign="top" class="title"><span class="rank">1.</span></td>
            <td valign="top" class="votelinks">
              <center>
                <a id='up_16851021' href='vote?id=16851021&amp;how=up&amp;goto=news'>
                  <div class='votearrow' title='upvote'></div>
                </a>
              </center>
            </td>
            <td class="title"><a href="https://www.nirsoft.net/about_nirsoft_freeware.html" class="storylink">NirSoft Freeware</a><span class="sitebit comhead"> (<a href="from?site=nirsoft.net"><span class="sitestr">nirsoft.net</span></a>)</span></td>
          </tr>
          <tr>
            <td colspan="2"></td>
            <td class="subtext">
              <span class="score" id="score_16851021">137 points</span> by <a href="user?id=valgaze" class="hnuser">valgaze</a> <span class="age"><a href="item?id=16851021">1 hour ago</a></span> <span id="unv_16851021"></span> | <a href="hide?id=16851021&amp;goto=news">hide</a> | <a href="item?id=16851021">17&nbsp;comments</a>
            </td>
          </tr>
          <tr class="spacer" style="height:5px"></tr>
          -->

          <% index = 1 %>
          <% @contributions.each do |contribution| %>
            <tr class='athing' id='<%= contribution.id %>'>
              <td align="right" valign="top" class="title"><span class="rank"><%= index %></span></td>
              <td valign="top" class="votelinks">
                <center>
                  <a id='up_<%= contribution.id %>' href='vote?id=<%= contribution.id %>&amp;how=up&amp;goto=news'>
                    <div class='votearrow' title='upvote'></div>
                  </a>
                </center>
              </td>
              <td class="title">
                <% unless contribution.url == '' %>
                  <a href="<%= contribution.url %>" class="storylink" target="_blank"><%= contribution.title %></a>
                  <!-- TODO: Implement filter by site and show only domain name -->
                  <span class="sitebit comhead"> (<a href="#"><span class="sitestr"><%= contribution.url %></span></a>)</span></td>
                <% else %>
                  <a href="#" class="storylink">NirSoft Freeware</a>
                  <span class="sitebit comhead"> (<a href="from?site=nirsoft.net"><span class="sitestr">nirsoft.net</span></a>)</span></td>
                <% end %>
            </tr>
            <tr>
              <td colspan="2"></td>
              <td class="subtext">
                <span class="score" id="score_<%= contribution.id %>">137 points</span> by <a href="user?id=valgaze" class="hnuser">valgaze</a>
                <span class="age"><a href="item?id=<%= contribution.id %>">1 hour ago</a></span> <span id="unv_<%= contribution.id %>"></span> | <a href="hide?id=<%= contribution.id %>&amp;goto=news">hide</a> | <a href="item?id=<%= contribution.id %>">17&nbsp;comments</a>
              </td>
            </tr>
            <tr class="spacer" style="height:5px"></tr>
            <% index += 1 %>
          <% end %>

          <!-- more button -->
          <tr class="morespace" style="height:10px"></tr>
          <tr>
            <td colspan="2"></td>
            <td class="title"><a href="news?p=2" class="morelink" rel="nofollow">More</a></td>
          </tr>
        </table>
      </td>
    </tr>
